CREATE TABLE DiceRolls (
    input TEXT NOT NULL,
    result INTEGER NOT NULL,
    details TEXT NOT NULL,
    rollTimeEpoch INTEGER NOT NULL
);

insertDiceRoll:
INSERT INTO DiceRolls(input, result, details, rollTimeEpoch)
VALUES(?, ?, ?, ?);

removeAllDiceRolls:
DELETE FROM DiceRolls;

selectAllDiceRolls:
SELECT DiceRolls.*
FROM DiceRolls
ORDER BY rollTimeEpoch DESC;